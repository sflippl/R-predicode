year <- sub("-.*", "", meta$Date)
note <- sprintf("R package version %s", meta$Version)

bibentry(bibtype = "Manual",
         title = "predicode: Provides an Extensible Interface to Implementations of Predictive Coding",
         author = c(person("Samuel", "Lippl")),
         year = 2019,
         note = note,
         url = "https://github.com/sflippl/predicode")
